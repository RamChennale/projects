<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<!-- Parallel testing to run methods in Parallel in this suite -->
<suite name="Test method Suite" parallel="methods" thread-count="2">
	<test name="Test-method test" group-by-instances="true">
		<classes>
			<class name="com.qa.parallel.ParallelMethodTest" />
		</classes>
	</test>
</suite>  
